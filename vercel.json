{
    "version": 2,
    "builds": [
      {
        "src": "app",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "env": {
      "DATABASE_URL": "mysql://your-database-username:your-database-password@aws.connect.psdb.cloud/sokofiti-admin?sslaccept=strict"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app/$1"
      }
    ]
  }
  